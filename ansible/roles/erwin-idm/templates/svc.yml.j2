apiVersion: v1
kind: Service
metadata:
  name: erwinidm-svc
  namespace: {{ NAMESPACE }}
  labels:
    app: erwinidm
spec:
  ports:
    - port: {{ ERWINIDM_PORT }}
      targetPort: {{ ERWINIDM_PORT }}
      protocol: TCP
      name: erwinidm
  selector:
    app: erwinidm
  clusterIP: None
  publishNotReadyAddresses: true